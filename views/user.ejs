  <script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/js/bootstrap.bundle.min.js"></script>
</head>

<body>
  <!-- Fixed navbar -->

  <div class="container">
    <div class="main-body">

      <!-- Breadcrumb -->
      <nav aria-label="breadcrumb" class="main-breadcrumb">

        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a style="  color: #231955;" class="nav-link" href="/logout">logout</a>
          </li>
          <li class="breadcrumb-item"><a style="  color: #231955;" href="/find">Find</a></li>
          <li class="breadcrumb-item active" aria-current="page"><a style="  color: #231955;" class="nav-link" href="/addpets">Add Pets</a></li>
        </ol>
      </nav>
      <!-- /Breadcrumb -->

      <div class="row gutters-sm">
        <div class="col-md-4 mb-3">
          <div class="card">
            <div class="card-body">
              <div class="d-flex flex-column align-items-center text-center">
                <img alt="Admin" class="rounded-circle" width="150" src="<%=profileData.Photo_URL%>" ;base64,>
                <div class="mt-3">
                  <h4><span> Name : </span> <%=profileData.fullName %></h4>
                  <p class="text-secondary mb-1"><span>Number :</span> <%=profileData.phone %></p>
                  <p class="text-muted font-size-sm"><span>Email : </span> <%= email %></p>
                  <button class="btn btn-primary"><a style=" color: #231955; text-decoration:none" href="/editpd/<%= profileData.login_userDB_id %>"> Mange profile </a></button>
                  <button class="btn btn-outline-primary"><a style=" color: #231955; text-decoration:none" href="/inbox/<%= profileData.login_userDB_id %>"> InBox </a></button>
                </div>
              </div>
            </div>
          </div>
          <div class="card mt-3">
            <ul class="list-group list-group-flush">

              <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                <%   if ( follower != null ){  %>
                <h6> <span>Matches By: </span><%= follower.followers_user.length %> </h6>
                <%  }else{%>
                <h6> <span>Matches By: </span> 0 </h6>

                <%  }  %>
                <%   if ( following != null ){  %>

                <h6> <span>My Matches: </span><%= following.following_users.length %> </h6>
                <%  }else{%>
                <h6> <span>followers: </span> 0 </h6>

                <%  }  %>
              </li>

            </ul>
          </div>
        </div>
        <div class="col-md-8">
          <div class="card mb-3">
            <div class="card-body">

              <div class="row">
                <% for(i=0; ads.length > i; i++){%>
                <% for(k=0; pics.length > k; k++){%>
                <%  if(i===k){ %>
                <div class="col-md-4">
                  <div class="blog-card blog-card-blog">
                    <div class="blog-card-image">
                      <a href="#"> <img class="img" src="<%=ads[i].petPic_Link%>" ;base64,> </a>
                      <div class="ripple-cont"></div>
                    </div>
                    <div class="blog-table">
                      <h6 class="blog-category blog-text-success"><i class="far fa-newspaper"></i> News</h6>
                      <h4 class="blog-card-caption">
                        <a href="#">>My pet : <% ads[i].SpecialMarks%> it's age is: <%=ads[i].age%> years and its Health like: <%=ads[i].Health%></a>
                      </h4>
                      <p class="blog-card-description"> Category : <%=ads[i].category%>.</p>
                      <div class="ftr">
                        <div class="author">
                          <a href="#"> <img src="<%=profileData.Photo_URL%>" alt="..." class="avatar img-raised"> <span>Lorem</span> </a>
                        </div>
                        <div class="stats"> <i class="far fa-clock"></i> <button class="btnCard" type="button" name="button"><a style="  color: #231955; text-decoration:none" href="/editads/<%= ads[i].userID %>"> Mange your Ads</a></button>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <%} %>
                <%} %>
                <%} %>
              </div>

            </div>
          </div>
        </div>



              <style type="text/css">
